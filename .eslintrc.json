{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "vue",
    "plugin:vue/recommended",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Vue": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "semi": false
      }
    ]
  }
}
